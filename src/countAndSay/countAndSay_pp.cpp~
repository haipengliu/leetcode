string vecToStr(vector<int> v)
{
  stringstream ss;
  for ( i = 0; i < v.size(); i++ )
  {
    ss<<v[i];
  }

  return ss.str();
}

string countAndSay(int n)
{
  string s;

  if ( n <= 0 )
  {
    return s;
  }

  if ( n == 1 )
  {
    s = "1";
    return s;
  }

  for ( int i = 2; i <= n; i++ )
  {

  }


}
